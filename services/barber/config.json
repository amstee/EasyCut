{
  "name": "barber",
  "port": 8080,
  "origins": [
    "http://easy-cut.ovh",
    "https://easy-cut.ovh",
    "http://localhost"
  ],
  "domain": "https://easy-cut.eu.auth0.com/",
  "api": "api/v2/",
  "tprefix": "auth0|",
  "oauth": "oauth/token",
  "security": "http://auth:8080",
  "sfile": "/run/secrets/auth0_api",
  "routes": [
    {
      "match_user": false,
      "route": "/status",
      "permissions": []
    },
    {
      "match_user": true,
      "route": "/create",
      "permissions": ["User"]
    }
  ]
}
